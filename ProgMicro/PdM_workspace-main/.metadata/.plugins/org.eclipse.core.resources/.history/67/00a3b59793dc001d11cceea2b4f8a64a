#include "main.h"
#include "mpu9265_SPI.h"

int main(void){

	APISysInit();

	uint8_t data = 0;
	setAccel2G();
	MPURead(28, &data, 1);
	setAccel4G();
	MPURead(28, &data, 1);
	setAccel8G();
	MPURead(28, &data, 1);
	setAccel16G();
	MPURead(28, &data, 1);

	uint16_t accel_x, accel_y;

	while (1)
	{
		accel_x = MPUReadAccelXAxis();
		accel_y = MPUReadAccelYAxis();
		HAL_Delay(100);
	}
}


