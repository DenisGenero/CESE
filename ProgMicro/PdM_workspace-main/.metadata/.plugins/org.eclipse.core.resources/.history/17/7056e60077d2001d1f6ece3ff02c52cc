
#include "main.h"
#include "SysConfig.h"
#include "API_uart.h"

int main(void)
{

  HAL_Init();
  uartInit();

  /* Configure the system clock to 180 MHz */
  SystemClock_Config();

  /* Initialize BSP Led for LED2 and LED3*/
  BSP_LED_Init(LED2);
  BSP_LED_Init(LED3);

  /* Infinite loop */
  while (1)
  {
	  BSP_LED_Toggle(LED3);
	  HAL_Delay(1000);
	  uartSendString("hola");
  }
}

