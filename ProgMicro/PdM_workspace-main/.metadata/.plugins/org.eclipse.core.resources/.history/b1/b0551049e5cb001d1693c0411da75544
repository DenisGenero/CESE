
/* Includes ------------------------------------------------------------------*/
#include "API_debounce.h"

/* Private functions ---------------------------------------------------------*/

/* @brief
 * Toggle LED1 state whenever a press state is detected
 * @param  None
 * @retval None*/
bool_t readKey(){
	bool_t flag = false;
	if(BSP_PB_GetState(BUTTON_USER)) flag = true;
	else flag = false;
	return(flag);

}

/* @brief
 * Toggle LED1 state whenever a press state is detected
 * @param  None
 * @retval None*/

void buttonPressed(){
	BSP_LED_Toggle(LED_GREEN);
}

